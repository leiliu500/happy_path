# Happy Path Database Environment Variables
# Copy this file to .env and update with your actual values

# Production Database Password
# IMPORTANT: Use a strong, unique password in production
POSTGRES_PASSWORD=your_secure_production_password_here

# Optional: Override default database settings
# POSTGRES_DB=happy_path
# POSTGRES_USER=happy_path_user

# Application Database Connection
# Development
DATABASE_URL_DEV=postgresql://happy_path_user:happy_path_dev_password@localhost:5433/happy_path_dev

# Production (update password)
DATABASE_URL=postgresql://happy_path_user:your_secure_production_password_here@localhost:5432/happy_path
