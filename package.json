{
  "name": "happy-path-workspace",
  "version": "1.0.0",
  "description": "Mental Health Wellness Platform - Monorepo",
  "scripts": {
    "start": "cd frontend && npm run start",
    "start:web": "cd frontend && npm run start:web",
    "start:frontend": "cd frontend && npm run start",
    "build": "cd frontend && npm run build",
    "build:web": "cd frontend && npm run build:web",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "test": "cd frontend && npm test",
    "lint": "cd frontend && npm run lint"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/leiliu500/happy_path.git"
  },
  "keywords": [
    "mental-health",
    "wellness",
    "react-native",
    "expo",
    "cross-platform"
  ],
  "author": "HappyPath Team",
  "license": "MIT"
}
